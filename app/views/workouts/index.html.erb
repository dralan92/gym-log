


<div class = "container">
	<div class = "row">
		<div class = "col">
			<% @workouts.each do |workout| %>
				<% if user_signed_in? %>
					<% if workout.user_id == current_user.id%>
						<div class="col">
							
							<a href = "/workouts/<%= workout.id %>"><span><%= workout.date %></span></a>
							<span><%= workout.workout %></span>
							<span><%= workout.mood %></span>
							<span><%= workout.length %></span>
						</div>
					<% end %>
				<% end %>
			<% end %>
		</div>
	</div>
</div>